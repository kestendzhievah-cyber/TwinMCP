{
  "name": "agentflow-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=8192' next build",
    "start": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "server": "node lib/express-server.js",
    "server:dev": "tsx lib/express-server.ts",
    "server:standalone": "tsx lib/server.ts",
    "mcp:init": "tsx lib/mcp/init.ts",
    "docs:generate": "tsx scripts/generate-docs.ts",
    "build:full": "npm install --legacy-peer-deps && npm run build",
    "install:legacy": "npm install --legacy-peer-deps",
    "clean": "rimraf .next out node_modules/.cache",
    "dev:clean": "npm run clean && npm install && npm run dev",
    "analyze": "cross-env ANALYZE=true NODE_OPTIONS='--max-old-space-size=8192' next build",
    "build:prod": "cross-env NODE_ENV=production NODE_OPTIONS='--max-old-space-size=8192' next build",
    "build:analyze": "cross-env ANALYZE=true NODE_OPTIONS='--max-old-space-size=8192' next build",
    "build:firebase": "cross-env NODE_ENV=production NODE_OPTIONS='--max-old-space-size=8192' next build",
    "optimize": "npm run build && next-sitemap",
    "predeploy": "npm run build:firebase",
    "deploy": "firebase deploy --only hosting",
    "deploy:ci": "npm ci --legacy-peer-deps && npm run build:firebase && firebase deploy --only hosting",
    "fix": "npm install --legacy-peer-deps && npm dedupe"
  },
  "dependencies": {
    "@auth/core": "^0.41.1",
    "@heroicons/react": "^2.2.0",
    "@modelcontextprotocol/sdk": "^1.20.2",
    "@prisma/client": "5.0.0",
    "@stripe/react-stripe-js": "^2.2.0",
    "@stripe/stripe-js": "^2.2.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vercel/speed-insights": "^1.2.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "firebase": "^10.7.1",
    "firebase-admin": "^13.5.0",
    "google-auth-library": "^10.5.0",
    "googleapis": "^167.0.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.300.0",
    "next": "^15.0.0",
    "next-auth": "^4.24.7",
    "nodemailer": "^7.0.12",
    "prisma": "5.0.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sonner": "^2.0.7",
    "stripe": "^19.1.0",
    "tailwind-merge": "^3.4.0",
    "typescript": "^5.3.3",
    "uuid": "^13.0.0"
  },
  "engines": {
    "node": "18.20.8",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/qrcode": "^1.5.6",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^9.8.8",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "postcss": "^7.0.39",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "zod": "^3.25.76"
  }
}
