{
  "name": "twinmcp-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "dev:ts": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "dev:debug": "nodemon --watch 'src/**/*.ts' --exec 'node --inspect -r ts-node/register' src/index.ts",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=8192' next build",
    "build:ts": "tsc --project tsconfig.json",
    "build:watch": "tsc --watch",
    "build:clean": "rm -rf dist && npm run build:ts",
    "start": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next start",
    "start:ts": "node dist/index.js",
    "start:dev": "ts-node src/index.ts",
    "lint": "next lint",
    "lint:check": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "pre-commit": "npm run lint:check && npm run format:check && npm test",
    "prepare": "husky",
    "mcp:init": "tsx lib/mcp/init.ts",
    "docs:generate": "tsx scripts/generate-docs.ts",
    "twinmcp:init": "tsx scripts/init-twinmcp.ts",
    "twinmcp:seed": "tsx prisma/seed.ts",
    "twinmcp:migrate": "npx prisma migrate dev --schema=prisma/schema",
    "twinmcp:generate": "npx prisma generate --schema=prisma/schema",
    "db:migrate": "npx prisma migrate dev --schema=prisma/schema",
    "db:generate": "npx prisma generate --schema=prisma/schema",
    "db:reset": "npx prisma migrate reset --schema=prisma/schema",
    "db:seed": "tsx scripts/db-seed.ts",
    "db:studio": "npx prisma studio --schema=prisma/schema",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:mcp": "docker-compose up -d mcp-server",
    "docker:all": "docker-compose up -d app mcp-server",
    "mcp:http": "node packages/mcp-server/dist/start-http-server.js",
    "mcp:http:dev": "npm run --prefix packages/mcp-server dev:http",
    "mcp:build": "npm run --prefix packages/mcp-server build",
    "start:all": "node scripts/start-all.js",
    "health:db": "tsx scripts/health-check.ts",
    "health:redis": "redis-cli ping",
    "vector:setup": "tsx scripts/vector-store-setup.ts",
    "vector:test": "npm test -- --testPathPattern=vector-store",
    "vector:health": "tsx -e \"import('./src/services/vector-store.service').then(s => new s.VectorStoreService().healthCheck().then(h => console.log('Healthy:', h)))\"",
    "vector:populate": "tsx scripts/populate-vector-store.ts",
    "setup:query-docs": "tsx scripts/setup-query-docs.ts",
    "storage:setup": "ts-node scripts/storage-setup.ts",
    "storage:test": "npm test -- --testPathPattern=storage",
    "storage:health": "tsx -e \"import('./src/services/document-storage.service').then(s => new s.DocumentStorageService().healthCheck().then(h => console.log('Healthy:', h)))\"",
    "build:full": "npm install --legacy-peer-deps && npm run build",
    "install:legacy": "npm install --legacy-peer-deps",
    "clean": "rimraf .next out node_modules/.cache",
    "dev:clean": "npm run clean && npm install && npm run dev",
    "analyze": "cross-env ANALYZE=true NODE_OPTIONS='--max-old-space-size=8192' next build",
    "build:prod": "cross-env NODE_ENV=production NODE_OPTIONS='--max-old-space-size=8192' next build",
    "build:analyze": "cross-env ANALYZE=true NODE_OPTIONS='--max-old-space-size=8192' next build",
    "build:firebase": "cross-env NODE_ENV=production NODE_OPTIONS='--max-old-space-size=8192' next build",
    "optimize": "npm run build && next-sitemap",
    "predeploy": "npm run build:firebase && npm run mcp:build",
    "deploy": "firebase deploy --only hosting",
    "deploy:ci": "npm ci --legacy-peer-deps && npm run build:firebase && firebase deploy --only hosting",
    "deploy:mcp": "gcloud builds submit --config=cloudbuild.mcp.yaml",
    "deploy:all": "npm run deploy && npm run deploy:mcp",
    "deploy:docker": "docker-compose up -d --build app mcp-server",
    "fix": "npm install --legacy-peer-deps && npm dedupe",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:report": "npx playwright show-report"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@aws-sdk/client-rds": "^3.966.0",
    "@aws-sdk/client-s3": "^3.966.0",
    "@aws-sdk/lib-storage": "^3.966.0",
    "@aws-sdk/s3-request-presigner": "^3.966.0",
    "@google-cloud/speech": "^6.7.0",
    "@google-cloud/text-to-speech": "^5.4.0",
    "@google-cloud/vision": "^4.3.2",
    "@heroicons/react": "^2.2.0",
    "@modelcontextprotocol/sdk": "^1.20.2",
    "@octokit/rest": "^22.0.1",
    "@octokit/webhooks": "^14.2.0",
    "@pinecone-database/pinecone": "^6.1.3",
    "@prisma/client": "^5.22.0",
    "@qdrant/js-client-rest": "^1.16.2",
    "@radix-ui/react-tabs": "^1.1.13",
    "@stripe/react-stripe-js": "^2.2.0",
    "@stripe/stripe-js": "^2.2.0",
    "@tiptap/core": "^2.8.0",
    "@tiptap/extension-collaboration": "^2.8.0",
    "@tiptap/extension-collaboration-cursor": "^2.8.0",
    "@tiptap/react": "^2.8.0",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.2",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dockerode": "^4.0.2",
    "file-type": "^21.3.0",
    "firebase": "^10.7.1",
    "firebase-admin": "^13.5.0",
    "fuse.js": "^7.0.0",
    "google-auth-library": "^10.5.0",
    "googleapis": "^167.0.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "leven": "^4.0.0",
    "lucide-react": "^0.300.0",
    "mime-types": "^3.0.2",
    "nanoid": "^5.0.7",
    "natural": "^8.0.1",
    "next": "^15.0.0",
    "nodemailer": "^7.0.12",
    "openai": "^6.17.0",
    "prisma": "^5.22.0",
    "puppeteer": "^24.35.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "string-similarity": "^4.0.4",
    "stripe": "^19.1.0",
    "tailwind-merge": "^3.4.0",
    "tar-stream": "^3.1.7",
    "tiktoken": "^1.0.22",
    "typescript": "^5.3.3",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "y-websocket": "^2.0.4",
    "yjs": "^13.6.20",
    "zod": "^3.25.76"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": "18.20.8",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@fastify/cors": "^11.2.0",
    "@jest/globals": "^30.2.0",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/canvas-confetti": "^1.9.0",
    "@types/dockerode": "^3.3.31",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^6.4.16",
    "@types/pg": "^8.16.0",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/sharp": "^0.32.0",
    "@types/string-similarity": "^4.0.2",
    "@types/supertest": "^6.0.3",
    "@types/tar-stream": "^3.1.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "autoprefixer": "^9.8.8",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "fastify": "^5.7.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-extended": "^7.0.0",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.11",
    "postcss": "^7.0.39",
    "prettier": "^3.7.4",
    "supertest": "^7.2.2",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6"
  }
}
