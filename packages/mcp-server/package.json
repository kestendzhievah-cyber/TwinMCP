    "directory": "packages/mcp-server"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@twinmcp/core": "^1.0.0",
    "zod": "^3.22.0",
    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "fast-levenshtein": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
